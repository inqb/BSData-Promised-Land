                <rule name="Sykaran Creed" id="1b4b-bcdb-be96-2418" hidden="false">
                  <description>Friendly units cannot make Cut Them Down reactions, but add 1 to all their melee weapons’ strength when making attacks against a unit while within that target’s front arc. This trait cannot be selected in conjunction with the Natural Hunters trait.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="69dd-3174-a6c2-20e4" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Wardens of the Fallen Kingdom" hidden="false" id="abd9-aa37-20a3-dc1f">
              <rules>
                <rule name="Wardens of the Fallen Kingdom" id="331b-b43d-4b45-946b" hidden="false">
                  <description>While wholly within their own deployment zone, units with this trait never accrue attrition points.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="d2c0-7817-bada-74b6" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Terror Troops" hidden="false" id="5c46-5328-9018-a24e">
              <rules>
                <rule name="Terror Troops" id="9f7c-b737-86c8-06c1" hidden="false">
                  <description>Enemy units within control range of any friendly *infantry* units subtract 1 from their morale characteristic.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="e36a-b300-3c2a-93d0" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="The Might of Murder" hidden="false" id="f569-9049-c2a8-96b5">
              <rules>
                <rule name="The Might of Murder" id="ca79-7e34-a564-9ac4" hidden="false">
                  <description>Friendly *infantry* units add 1 to their melee weapons’ strength when targeting enemy models with a health characteristic of 10 or greater.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="221b-f7fe-49b3-25f0" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Storm of Steel" hidden="false" id="2bb7-5a3c-ddec-dd86">
              <rules>
                <rule name="Storm of Steel" id="d31d-f474-8969-a019" hidden="false">
                  <description>Friendly *vehicle* units add 2 to their melee weapons’ strength until the end of any turn in which they make a successful charge.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="5517-ce07-987c-d3a3" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Cull the Weak" hidden="false" id="1716-6b25-8f09-bc96">
              <rules>
                <rule name="Cull the Weak" id="14f5-c486-f605-d5f0" hidden="false">
                  <description>Friendly *infantry* units add 1 to their melee weapons’ dice characteristic when targeting enemy units containing 10 or more models.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="f8fd-5392-65a0-4f95" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Relentless Advance" hidden="false" id="9a63-295f-3e33-ff14">
              <rules>
                <rule name="Relentless Advance" id="172c-4baf-95fd-bbcb" hidden="false">
                  <description>After finishing an Advance action, friendly *infantry* units can make snap-fire shots. A unit making snap-fire shots can attack as if they are making a Shoot action. Natural snap-fire shot attack rolls of less than 10 always fail, and snap-fire shots can never inflict critical hits.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="d18e-c30c-2a28-b85e" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Witness to Unearthly Horrors" hidden="false" id="02fb-f00f-64e7-2443">
              <rules>
                <rule name="Witness to Unearthly Horrors" id="1d35-6a37-32c4-c1cd" hidden="false">
                  <description>Friendly *infantry* units add 1 to all of their attrition test rolls.</description>
                </rule>
              </rules>
              <constraints>
                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="3213-cac6-afd6-834b" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="First to the Fray" hidden="false" id="579f-3894-c93f-ce4c">
              <rules>
                <rule name="First to the Fray" id="e3d0-e8c0-630c-53b7" hidden="false">
