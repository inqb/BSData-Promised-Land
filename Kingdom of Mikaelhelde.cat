    <profile name="Butcher&apos;s Blade" typeId="2857-5ab3-db0f-64a1" typeName="Melee Weapon" hidden="false" id="134e-5b2c-cddd-d337">
      <characteristics>
        <characteristic name="Dice" typeId="e736-f6b3-0474-ddb8">2</characteristic>
